{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "declaration": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@ant-design/pro-components": ["src"]
    }
  },
  "include": [
    ".dumirc.ts",
    "vitest.config.ts",
    "tests/**/**/*.tsx",
    "tests/**/**/*.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "typings.d.ts",
    "src/**/*",
    "demos/**/*"
  ]
}
