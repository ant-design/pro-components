{
  "name": "@ant-design/pro-layout",
  "version": "6.2.5",
  "description": "ant-design-pro layout, easy to use pro scaffolding.",
  "keywords": [
    "admin",
    "ant",
    "ant-design",
    "ant-design-pro",
    "antd",
    "design",
    "layout",
    "menu",
    "pro"
  ],
  "homepage": "https://github.com/ant-design/ant-design-pro-layout#readme",
  "bugs": {
    "url": "https://github.com/ant-design/ant-design-pro-layout/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ant-design/ant-design-pro-layout"
  },
  "license": "MIT",
  "author": "qixian.cs@outlook.com",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "/es",
    "/dist"
  ],
  "scripts": {
    "build": "father-build && webpack",
    "doc": "cd ./example && umi dev",
    "lint": "npm run lint-eslint && npm run lint:style",
    "lint-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty  && npm run lint:style",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prepublishOnly": "npm run test && npm run build && np --no-cleanup --yolo --no-publish --any-branch",
    "prettier": "prettier -c --write \"**/**\"",
    "site": "dumi build && gh-pages -d ./dist",
    "site_build": "dumi build",
    "start": "dumi dev",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "tsc": "tsc -p tsconfig-check.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@umijs/route-utils": "^1.0.5",
    "antd": "^4.2.0",
    "classnames": "^2.2.6",
    "hash.js": "^1.1.7",
    "history": "^4.10.1",
    "lodash.isequal": "^4.5.0",
    "memoize-one": "^5.1.1",
    "omit.js": "^2.0.2",
    "path-to-regexp": "2.4.0",
    "qs": "^6.9.0",
    "rc-resize-observer": "^0.2.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-responsive": "^8.0.1",
    "react-router-dom": "5.1.2",
    "unstated-next": "^1.1.0",
    "use-json-comparison": "^1.0.3",
    "use-media-antd-query": "^1.0.1",
    "use-merge-value": "^1.0.1",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@ant-design/compatible": "^1.0.1",
    "@ant-design/dark-theme": "^2.0.2",
    "@ant-design/pro-skeleton": "^1.0.0-beta.2",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@material-ui/core": "^4.8.2",
    "@testing-library/react": "^10.0.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.10.3",
    "@types/history": "^4.7.3",
    "@types/jest": "^26.0.0",
    "@types/lodash.isequal": "^4.5.5",
    "@types/memoize-one": "^5.1.2",
    "@types/qs": "^6.9.0",
    "@types/react": "^16.9.17",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.1",
    "@types/react-helmet": "^5.0.15",
    "@types/react-intl": "^3.0.0",
    "@types/react-responsive": "^8.0.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/warning": "^3.0.0",
    "@umijs/fabric": "^2.0.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.12.2",
    "cross-env": "^7.0.0",
    "dumi": "^1.0.13",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^7.2.0",
    "father-build": "^1.18.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.0.0",
    "jsdom": "^16.2.0",
    "jsdom-global": "^3.0.2",
    "less-loader": "^6.0.0",
    "lint-staged": "^10.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "np": "^6.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-github-btn": "^1.1.1",
    "react-markdown": "^4.3.0",
    "slash2": "^2.0.0",
    "stylelint": "^13.0.0",
    "terser-webpack-plugin": "^3.0.2",
    "typescript": "^3.8.3",
    "umi": "^3.0.0",
    "umi-plugin-githubpages": "^2.0.0",
    "umi-test": "^1.9.6",
    "umi-types": "^0.5.11",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {
    "antd": "^4.x",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
