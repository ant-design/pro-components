{
  "name": "@ant-design/pro-layout",
  "version": "4.3.2",
  "description": "ant-design-pro layout, easy to use pro scaffolding.",
  "files": [
    "/dist"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi-lib build",
    "format-imports": "import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "lint": "npm run lint-eslint && npm run lint:ts && npm run lint:style && npm run lint:prettier",
    "lint-eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ./_test_ && npm run lint:style",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix  'src/**/*.less'  --syntax less",
    "prepublishOnly": "umi-lib build",
    "prettier": "node ./scripts/prettier.js",
    "start": "cd ./example && cross-env APP_TYPE=site umi dev",
    "test": "umi test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,ts,tsx,json,jsx,less}": [
      "node ./scripts/lint-prettier.js",
      "git add"
    ],
    "**/*.{js,ts,tsx,jsx}": "npm run lint-staged:js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "antd": "^3.19.0",
    "classnames": "^2.2.6",
    "history": "^4.9.0",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "memoize-one": "^5.0.4",
    "moment": "^2.22.2",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^3.0.0",
    "qs": "^6.7.0",
    "react": "^16.8.5",
    "react-container-query": "^0.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.7.0",
    "react-media-hook2": "^1.0.2",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.1",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/memoize-one": "^4.1.1",
    "@types/react": "^16.8.18",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.8.4",
    "@types/react-intl": "^2.3.17",
    "@types/react-router-dom": "^4.3.2",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.11.0",
    "chalk": "^2.4.2",
    "check-prettier": "^1.0.1",
    "cross-env": "^5.2.0",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.9.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-typescript": "^1.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-unicorn": "^8.0.1",
    "husky": "^2.3.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest-puppeteer": "^4.1.1",
    "jsdom": "^15.0.0",
    "jsdom-global": "^3.0.2",
    "less": "^3.9.0",
    "lint-staged": "^8.1.1",
    "prettier": "^1.17.0",
    "puppeteer": "^1.12.1",
    "slash2": "^2.0.0",
    "stylelint": "^10.0.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^3.0.0",
    "umi": "^2.6.14",
    "umi-library": "^1.3.0",
    "umi-types": "^0.3.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
